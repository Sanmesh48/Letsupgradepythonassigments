   "source": [
    "print(\"Enter String: \", end=\"\")\n",
    "text = input()\n",
    "\n",
    "vowels = ['a', 'e', 'i', 'o', 'u', 'A', 'E', 'I', 'O', 'U']\n",
    "newtext = \"\"\n",
    "\n",
    "for c in text:\n",
    "    if c not in vowels:\n",
    "        newtext = newtext + c\n",
    "\n",
    "text = newtext\n",
    "print(\"\\nWithout Vowels =\", text)"
   ]
